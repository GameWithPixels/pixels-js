{
  "expo": {
    "name": "Pixels Toolbox",
    "slug": "pixels-toolbox",
    "owner": "gamewithpixels",
    "version": "1.6.0",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/deab8983-a21f-4c31-9861-41762637a793"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      },
      "bundleIdentifier": "com.systemic.pixelstoolbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#222222"
      },
      "package": "com.systemic.pixelstoolbox",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./withAndroidPermissions",
      "expo-localization",
      "react-native-vision-camera",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.7.0",
            "flipper": "0.182.0"
          }
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "systemic-games",
            "project": "pixels-toolbox",
            "authToken": "8b0ed4f628dd4e6c9f8095bd698df89ae4ccf6fc0b8045e9add36b9ef4ab6df7"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "deab8983-a21f-4c31-9861-41762637a793"
      }
    },
    "jsEngine": "hermes"
  }
}
