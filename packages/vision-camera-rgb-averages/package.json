{
  "name": "@systemic-games/vision-camera-rgb-averages",
  "version": "1.3.0",
  "description": "Vision Camera Frame Processor computing RGB averages.",
  "main": "dist/commonjs/index.cjs",
  "module": "dist/module/index.mjs",
  "types": "dist/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "exports": {
    ".": {
      "types": "./dist/typescript/index.d.ts",
      "import": "./dist/module/index.mjs",
      "require": "./dist/commonjs/index.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "bob build",
    "publish": "release-it"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "react-native",
    "android",
    "vision-camera",
    "camera",
    "rgb average"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:GameWithPixels/pixels-js.git"
  },
  "author": "Systemic Games (https://gamewithpixels.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GameWithPixels/pixels-js/issues"
  },
  "homepage": "https://github.com/GameWithPixels/pixels-js/tree/main/packages/vision-camera-rgb-averages",
  "devDependencies": {
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-builder-bob": "^0.25.0",
    "react-native-vision-camera": "4.4.1",
    "react-native-worklets-core": "^1.3.3",
    "release-it": "^17.2.0"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vision-camera": "*",
    "react-native-worklets-core": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": false
    },
    "plugins-disabled": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNVisionCameraRgbAveragesSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "create-react-native-library": {
    "type": "module-mixed",
    "languages": "cpp",
    "version": "0.38.1"
  }
}
