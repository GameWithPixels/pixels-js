{
  "name": "@systemic-games/vision-camera-rgb-averages",
  "version": "1.0.0",
  "description": "Vision Camera Frame Processor computing RGB averages.",
  "main": "dist/commonjs/index",
  "module": "dist/module/index",
  "types": "dist/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "vision-camera-rgb-averages.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "bob build",
    "release": "release-it --only-version"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "react-native",
    "android",
    "vision-camera",
    "camera",
    "rgb average"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:GameWithPixels/pixels-js.git"
  },
  "author": "Systemic Games (https://gamewithpixels.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GameWithPixels/pixels-js/issues"
  },
  "homepage": "https://github.com/GameWithPixels/pixels-js#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^29.1.2",
    "@types/react": "~18.0.0",
    "@types/react-native": "0.69.6",
    "jest": "^29.1.2",
    "react": "18.0.0",
    "react-native": "0.69.7",
    "react-native-builder-bob": "^0.20.0",
    "react-native-vision-camera": "~2.14.1",
    "release-it": "^15.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vision-camera": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
